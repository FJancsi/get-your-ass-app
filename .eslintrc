{
    "env": {
      "node": true,
      "mocha": true,
      "es2020": true
    },
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "settings": {
      "import/extensions": [
        ".js",
        ".jsx",
        ".ts",
        ".tsx"
      ],
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "plugins": ["prettier"],
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "rules": {
      "no-console": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ],
      "arrow-body-style": "off",
      "prefer-arrow-callback": "off",
      "consistent-return": "off",
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "mjs": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
    }
  }
  